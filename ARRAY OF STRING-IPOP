#include <stdio.h>
#include<string.h>
void main()
{
    char a[10][10];
    int b;
    printf("Enter no of names");
    scanf("%d",&b);
    printf("enter the names");
    for(int i=1;i<b;i++)
    {
        scanf("%s",a[i]);
    }
    printf("th legends are:");
    for(int i=0;i<b;i++)
    {
        printf("\n %d  %s ",i+1,a[i]);
    }
}

#include<stdio.h>
int factorial(int n);
int factorial(int n)
{
    int result=1;
    while(n==0)
    {
        return result;
    }
    result=n*factorial(n-1);
    return result;
}
int main()
{
    int p;
    printf("enter number");
    scanf("%d",&p);
    printf("the fact of %d  is %d",p,factorial(p));
}

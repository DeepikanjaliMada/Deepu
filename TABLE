#include <stdio.h>
void main()
{
    int num;
    printf("enter the value");
    scanf("%d",&num);
    if(num%2==0)
    {
        for(int i=1;i<15;i++)
        {
            printf("%d*%d=%d",i,num,i*num);
            printf("\n");
        }
    }    
    else    
    {
        for(int j=1;j<10;j++)
        {
            printf("%d*%d=%d",j,num,j*num);
            printf("\n");
        }
    }
}
